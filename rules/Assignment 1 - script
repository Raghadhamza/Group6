# BIManalyst group 06
import ifcopenshell
from bonsai.bim.ifc import IfcStore
file = IfcStore.get_file()

Window = file.by_type('IfcWindow')
print(len(Window))

def checkRule(model):

Window = model.by_type('IfcWindow')

result=f"Window: {len(Window)}"

Plate = file.by_type('IfcPlate')
print("Plate"len(Plate))


Plate = model.by_type('IfcPlate')

result=f"Plate: {len(Plate)}"

CurtainWall = file.by_type('IfcCurtainWall')
print("Curtain walls", len(CurtainWall))

def checkRule(model):

CurtainWall = model.by_type('IfcCurtainWall')

result=f"CurtainWall: {len(CurtainWall)}"
